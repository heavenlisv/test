stock SendUnitMessage(playerid,color, str[])
{
	foreach(new users: Player)
	{
		if(PlayerInfo[playerid][pMember] == PlayerInfo[users][pMember])
		{
			if(PlayerInfo[playerid][pUnit] == PlayerInfo[users][pUnit] && GetPVarInt(playerid,"users") != -1)
			{
				SendClientMessage(users, color, str);
			}
		}
	}
}

stock SendMemberMessage(playerid,color, str[])
{
	foreach(new users: Player)
	{
		if(PlayerInfo[playerid][pMember] == PlayerInfo[users][pMember])
		{
			SendClientMessage(users, color, str);
		}
	}
}

stock SMM(member,color, str[])
{
	foreach(new user: Player)
	{
		if(member == PlayerInfo[user][pMember])
		{
			SendClientMessage(user, color, str);
		}
	}
}

