stock StartReconForPlayer(playerid, specid)
{
    printf("ShowRecon 0");
    if(PlayerInfo[playerid][pAdmin] < 1 || !IsPlayerConnected(specid)) return true;
    if(GetPVarInt(playerid, "spectate_samp") == 0)
    {
        new Float:Pos[3];
        GetPlayerPos(playerid, Pos[0], Pos[1], Pos[2]);
        SetPVarFloat(playerid, "PosSpecX", Pos[0]);
        SetPVarFloat(playerid, "PosSpecY", Pos[1]);
        SetPVarFloat(playerid, "PosSpecZ", Pos[2]);
        SetPVarInt(playerid, "SpecInt", GetPlayerInterior(playerid));
        SetPVarInt(playerid, "SpecVT", GetPlayerVirtualWorld(playerid));
    }
    printf("ShowRecon");
    ShowReconPlayerInfoForPlayer(playerid);
    TogglePlayerSpectating(playerid, true);
    SetPVarInt(playerid, "SpecID", specid);
    SetPVarInt(playerid, "spectate_samp", 1);
    SetPlayerInterior(playerid, GetPlayerInterior(specid));
    SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(specid));
    printf("ShowRecon 1");
    if(!IsPlayerInAnyVehicle(specid)) return PlayerSpectatePlayer(playerid, specid);
    else return PlayerSpectateVehicle(playerid, GetPlayerVehicleID(specid));

}
