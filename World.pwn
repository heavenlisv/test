public UpdateReconInfoPanel(playerid)
{
	printf("Obnova");
    if(!IsPlayerConnected(GetPVarInt(playerid, "SpecID"))) return StopRecon(playerid, "~w~RECON ~r~OFF~n~PLAYER DISCONNECT");
    printf("Obnova 1");
    new text[32], Float:health, Float:vhealth, Float:armour;
    GetPlayerHealth(GetPVarInt(playerid, "SpecID"), health);
    GetVehicleHealth(GetPlayerVehicleID(GetPVarInt(playerid, "SpecID")), vhealth);
    GetPlayerArmour(GetPVarInt(playerid, "SpecID"), armour);
    format(text, sizeof(text), "~w~%s", PlayerInfo[GetPVarInt(playerid, "SpecID")][pName]);
    PlayerTextDrawSetString(playerid, ReconNick[playerid], text);
    format(text, sizeof(text), "(~w~%d)",GetPVarInt(playerid, "SpecID"));
    PlayerTextDrawSetString(playerid, ReconID[playerid], text);
    format(text, sizeof(text), "~r~]Armour: ~w~%.0f", floatround(armour, floatround_round));
    PlayerTextDrawSetString(playerid, ReconArmour[playerid], text);
    format(text, sizeof(text), "~g~]HP: ~w~%d", floatround(health, floatround_round));
    PlayerTextDrawSetString(playerid, ReconHealth[playerid], text);
    format(text, sizeof(text), "~g~]CARHP: ~w~%.0f", floatround(vhealth, floatround_round));
    PlayerTextDrawSetString(playerid, ReconCarHealth[playerid], text);
    format(text, sizeof(text), "~r~]MONEY: ~w~%d", GetPlayerMoney(GetPVarInt(playerid, "SpecID")));
    PlayerTextDrawSetString(playerid, ReconMoney[playerid], text);
    format(text, sizeof(text), "~b~]Ammo: ~w~%d", GetPlayerAmmo(GetPVarInt(playerid, "SpecID")));
    PlayerTextDrawSetString(playerid, ReconAmmo[playerid], text);
    format(text, sizeof(text), "~p~]WARNS: ~w~%d", PlayerInfo[GetPVarInt(playerid, "SpecID")][pWarn]);
    PlayerTextDrawSetString(playerid, ReconWarns[playerid], text);
    format(text, sizeof(text), "~y~]Ping: ~w~%d", GetPlayerPing(GetPVarInt(playerid, "SpecID")));
    PlayerTextDrawSetString(playerid, ReconPing[playerid], text);
    format(text, sizeof(text), "~b~]Speed: ~w~%d", SpeedVehicle(GetPVarInt(playerid, "SpecID")));
    PlayerTextDrawSetString(playerid, ReconSpeed[playerid], text);
    printf("Obnova 2");
    return true;
}

stock StopRecon(playerid, reason[])
{
    if(PlayerInfo[playerid][pAdmin] < 1 || GetPVarInt(playerid, "spectate_samp") == 0) return true;
    HideReconPlayerInfoForPlayer(playerid);
    TogglePlayerSpectating(playerid, false);
    DeletePVar(playerid, "SpecID");
    GameTextForPlayer(playerid, reason, 1500, 4);
    KillTimer(ReconTimer[playerid]);
    return true;
}
