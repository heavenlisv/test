stock GetHouseZone(houid)
{
	new zone[32],bool:getzone;
	for(new i=0; i<sizeof(gZoneInfo); i++)
	{
		if(HouseInfo[houid][ExitX] >= gZoneInfo[i][SAZONE_AREA][0] && HouseInfo[houid][ExitY] <= gZoneInfo[i][SAZONE_AREA][3]
				&& HouseInfo[houid][ExitX] >= gZoneInfo[i][SAZONE_AREA][1] && HouseInfo[houid][ExitY] <= gZoneInfo[i][SAZONE_AREA][4])
		{
			settext(zone, gZoneInfo[i][SAZONE_NAME]);
			getzone = true;
			break;
		}
	}
	if(!getzone) zone = "Неизвестно";
	return zone;
}

