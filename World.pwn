					format(query,sizeof(query),  "{1e90ff}%s {1e90ff}предлагает Вам купить дом №%d за {34c924}%d$", PlayerInfo[playerid][pName],PlayerInfo[playerid][pHouse], price);
					SendClientMessage(giveplayerid, COLOR_WHITE, query);
					SendClientMessage(giveplayerid, CWHITE, "Нажмите {03c03c}Y {FFFFFF}чтобы согласиться или {ff7518}N {FFFFFF}для отказа.");
