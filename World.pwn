	SaveStoreBallas();
	SaveStoreAztec();
	SaveStoreVagos();
	SaveStoreRifa();
	SaveGangsStores();
	SaveMafiaStores();
	SaveArmyStores();
	SaveMiner();
	SaveDrugs();

	foreach(new a: Player)
	{
		if(IsPlayerConnected(a))
		{
			if(Logged[a] == true)
			{
				SaveAll(a);
				OnPlayerSave(a);
			}
		}
	}
	if(loadfs == true)
	{
		mysql_query(dbHandle,"DELETE FROM election WHERE 1",false);
		mysql_query(dbHandle,"TRUNCATE TABLE `election`",false);
		Delete3DTextLabel(TextAS[0]);
		Delete3DTextLabel(TextAS[1]);
