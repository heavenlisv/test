	if(strcmp(cmd, "/sellmyhome", true) == 0)
	{
		if(PlayerInfo[playerid][pHouse] == 999) return SendClientMessage(playerid, COLOR_GRAD1, "У Вас нет дома.");
		tmp = strtok(cmdtext, idx);
		new price;
		if(!strlen(tmp)) return SendClientMessage(playerid, COLOR_GRAD2, "Используйте: /sellmyhome [id игрока] [цена]");
		giveplayerid = ReturnUser(tmp);
		tmp = strtok(cmdtext, idx);
		price = strval(tmp);
		if(!strlen(tmp)) return SendClientMessage(playerid, COLOR_GRAD2, "Используйте: /sellmyhome [id игрока] [цена]");
		if (PlayerInfo[playerid][pHouse] < 999)
		{
			if(IsPlayerConnected(giveplayerid))
			{
				if(giveplayerid != INVALID_PLAYER_ID)
				{
					if(GetPVarInt(playerid,"yes") == 1) return SendClientMessage(playerid,COLOR_GRAD2, "Вы не можете использовать это в данный момент, отзовите своё предыдущее предложение {0099FF}/cancel");
					if(playerid == giveplayerid) return SendClientMessage(playerid, COLOR_GRAD1, "[{FF0000}Error{B4B5B7}] Неверный id.");
					if(HouseInfo[PlayerInfo[playerid][pHouse]][hCar] > 0) return SendClientMessage(playerid, COLOR_GRAD1, "Сначала продайте домашний транспорт!");
					if(GetDistanceBetweenPlayers(playerid, giveplayerid) > 5 || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(giveplayerid)) return SendClientMessage(playerid, COLOR_GRAD1, "[{FF0000}Error{B4B5B7}] Игрок слишком далеко от Вас.");
					if(price < HouseInfo[PlayerInfo[playerid][pHouse]][hPrice]) { SendClientMessage(playerid, COLOR_GRAD1, "Цена не может быть меньше государственной!"); return true; }
					if(PlayerInfo[giveplayerid][pMoney] < price) return SendClientMessage(playerid, COLOR_GRAD1, "У игрока нет такой суммы денег.");
					if(PlayerInfo[giveplayerid][pHouse] < 999) return SendClientMessage(playerid, COLOR_GRAD1, "У этого игрока уже есть дом.");
					if(!IsPlayerInRangeOfPoint(playerid,20.0,HouseInfo[PlayerInfo[playerid][pHouse]][ExitX],HouseInfo[PlayerInfo[playerid][pHouse]][ExitY],HouseInfo[PlayerInfo[playerid][pHouse]][ExitZ])) return SendClientMessage(playerid, COLOR_GRAD1, "Вы должны находиться у дома!");
					SetPVarInt(giveplayerid,"sellmyhome",1);
					SetPVarInt(playerid,"yes",1);
					yesid[playerid] = giveplayerid;
					GetPlayerTransfer1[giveplayerid] = playerid;
					GetPlayerOption1[giveplayerid] = price;
					format(query,sizeof(query), "Вы предложили игроку {9DDAF2}%s{1e90ff} купить Ваш дом (№%d) за {00CC00}%d$", PlayerInfo[giveplayerid][pName],PlayerInfo[playerid][pHouse], price);
