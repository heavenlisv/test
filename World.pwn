stock CheckAdminCombo(playerid)
{
	if(PlayerInfo[playerid][pAdmin] > 0)
	{
	    if(AdminCombo[playerid][pTimer] > 0)
	    {
			if(AdminCombo[playerid][pJail] > 4 || AdminCombo[playerid][pWarn] > 4 || AdminCombo[playerid][pBan] > 3 || AdminCombo[playerid][pKick] > 4 || AdminCombo[playerid][pSKick] > 5 || AdminCombo[playerid][pMute] > 4 || AdminCombo[playerid][pRban] > 3 || AdminCombo[playerid][pUnRban] > 3 || AdminCombo[playerid][pMsg] > 6 || AdminCombo[playerid][pSBan] > 2 || AdminCombo[playerid][pCC] > 2)
			{
	        	PlayerInfo[playerid][pAdmin] = 0;
	        	SaveAll(playerid);
	        	new string[128];
	        	format(string, sizeof(string), "[A] %s[%d] был снят с поста администратора системой безопасности. Возможно взломан",PlayerInfo[playerid][pName],playerid);
	        	SMA(5,0xFF0000AA, string);
	        	ClearAdminCombo(playerid);
	        }
		}
	}
	if(PlayerInfo[playerid][pLeader] > 0)
	{
	    if(AdminCombo[playerid][pTimer] > 0)
	    {
			if(AdminCombo[playerid][pNews] > 5)
			{
			 	new string[128];
	        	format(string, sizeof(string), "[A] %s[%d] был снят с поста лидера системой безопасности. Возможно взломан",PlayerInfo[playerid][pName],playerid);
	        	SMA(5,0xFF0000AA, string);
	        	WarnAC(playerid);
	        	SaveAll(playerid);
	        	ClearAdminCombo(playerid);
	        }
		}
	}
}


